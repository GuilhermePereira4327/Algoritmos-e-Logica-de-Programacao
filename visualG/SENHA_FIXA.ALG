Algoritmo "senha_fixa"

Var

  senha, acesso : inteiro

Inicio

    acesso <- 2002

    escreva("Digite a senha: ")
    leia(senha)
    
    enquanto acesso <> senha faca
       escreva("Senha Invalida! Tente novamente: ")
       leia(senha)
    fimenquanto
    
    escreval("Acesso permitido!")

Fimalgoritmo